{
  "name": "Generic HC32F460 Series",
  "url": "https://www.hdsc.com.cn/Category83-1487",
  "vendor": "HUADA Semiconductor",
  "frameworks": [
    "arduino",
    "ddl"
  ],
  "build": {
    "variant": "generic_hc32f460",
    "ld_args": {
      "flash_start": "0x00000000",
      "flash_size": "256K"
    },
    "cpu": "cortex-m4",
    "f_cpu": "8000000"
  },
  "upload": {
    "maximum_size": 262144,
    "maximum_ram_size": 192512
  },
  "debug": {
    "svd_path": "HDSC_HC32F460.svd",
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-f",
            "interface/cmsis-dap.cfg",
            "-f",
            "target/stm32f4x.cfg",
            "-c",
            "gdb_memory_map disable"
          ]
        }
      }
    }
  }
}